{
	"compilerOptions": {
		"strict": true,
		"noEmit": true,
		"target": "es6",
		"allowJs": true,
		"sourceMap": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"node_modules"
	]
}
