{
  "private": true,
  "name": "music-app",
  "version": "1.0.0",
  "description": "music-app",
  "deprecated": false,
  "type": "module",
  "main": "src/server/index.js",
  "scripts": {
    "server": "node -r dotenv/config",
    "delete-build": "rm -r -f src/server/build",
    "sync": "git checkout . && git remote update && git pull",
    "dev-server": "node_modules/.bin/nodemon --trace-warnings -r dotenv/config",
    "push": "clear && git status && git add -A && git commit -m \"Update\" && git push -u origin master",
    "dev-client": "node_modules/.bin/webpack-dev-server --env.NODE_ENV=local --config webpack.config.cjs -r dotenv/config",
    "build": "clear && npm run delete-build && \"node_modules/.bin/webpack\" --env.NODE_ENV=local --config webpack.config.cjs",
    "dev": "clear && npm run delete-build && clear && \"node_modules/.bin/concurrently\" \"npm run dev-client\" \"npm run dev-server\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olyop/music-app.git"
  },
  "keywords": [
    "react",
    "lodash",
    "apollo",
    "graphql",
    "express",
    "nodejs",
    "sql",
    "postgresql",
    "typescript",
    "webpack",
    "eslint"
  ],
  "author": {
    "name": "Oliver Plummer",
    "email": "oliver.plummer@outlook.com"
  },
  "license": "MIT",
