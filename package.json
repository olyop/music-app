{
  "private": true,
  "name": "music-app",
  "version": "1.0.0",
  "description": "music-app",
  "deprecated": false,
  "type": "module",
  "scripts": {
    "delete-build": "rm -r -f src/server/build",
    "sync": "git checkout . && git remote update && git pull",
    "server": "clear && node --require dotenv/config src/server/index.js",
    "dev-server": "node_modules/.bin/nodemon --require dotenv/config src/server/index.js",
    "dev-client": "node_modules/.bin/webpack-dev-server --config webpack.config.js --env.mode development",
    "push": "clear && git status && git add -A && git commit -m \"Update\" && git push -u origin master && exit",
    "build": "npm run delete-build && clear && \"node_modules/.bin/webpack\" --config webpack.config.js --env.mode production",
    "dev": "npm run delete-build && clear && \"node_modules/.bin/concurrently\" \"npm run dev-client\" \"npm run dev-server\" && clear"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olyop/music-app.git"
  },
