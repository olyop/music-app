	"scripts": {
		"dev": "clear && npm run delete-build && npm run build-server && npx concurrently \"npm:dev-server\" \"npm:dev-client\" \"npm:dev-nodemon\"",
		"dev-server": "npx tsc --watch",
		"dev-client": "npx cross-env NODE_ENV=dev TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack-dev-server --env.isDev",
		"dev-nodemon": "npx nodemon -r dotenv/config dist/index.js",
		"build": "npm run build-server && npm run build-client",
		"build-server": "npx tsc",
		"build-client": "npx cross-env NODE_ENV=pro TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack",
		"delete-build": "rm -r -f dist && rm -r -f .awcache",
		"sync": "git checkout . && git remote update && git pull",
		"push": "clear && git status && git add -A && git commit -m \"Update\" && git push -u origin master"
	},
