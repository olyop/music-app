	"scripts": {
		"dev": "npm run dev-init && npx concurrently \"npm:dev-copy\" \"npm:dev-server\" \"npm:dev-nodemon\" \"npm:dev-client\" \"npm:dev-upload\"",
		"client-dev": "npm run dev-init && npx concurrently \"npm:dev-copy\" \"npm:dev-server\" \"npm:dev-nodemon\" \"npm:dev-client\"",
		"upload-dev": "npm run dev-init && npx concurrently \"npm:dev-copy\" \"npm:dev-server\" \"npm:dev-nodemon\" \"npm:dev-upload\"",
		"server-dev": "npm run dev-init && npx concurrently \"npm:dev-copy\" \"npm:dev-server\" \"npm:dev-nodemon\"",
		"dev-init": "clear && npm run init && npm run build-server && npm run build-copy",
		"dev-nodemon": "npx nodemon -r dotenv/config dist",
		"dev-server": "npx tsc --project tsconfig.server.json --watch",
		"dev-copy": "npx babel src/server -d dist -D --quiet -w",
		"dev-client": "npx cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack-dev-server --config webpack.client.ts",
		"dev-upload": "npx cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack-dev-server --config webpack.upload.ts",
		"build-host": "clear && npm run build && clear && npm run host",
		"host": "node -r dotenv/config dist",
		"build": "npm run init && npm run build-server && npm run build-copy && npm run build-client && npm run build-upload",
		"build-copy": "npx babel src/server -d dist -D --quiet",
		"build-server": "npx tsc --project tsconfig.server.json",
		"build-client": "npx cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack --config webpack.client.ts",
		"build-upload": "npx cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack --config webpack.upload.ts",
		"init": "clear && npm run kill-port && npm run delete-build",
		"delete-build": "rm -r -f dist",
		"kill-port": "npx kill-port 3000 3001 3002",
		"sync": "git checkout . && git remote update && git pull",
		"push": "clear && git status && git add -A && git commit -m \"Update\" && git push -u origin master",
		"publish-common": "npx tsc --outDir src/common --project tsconfig.common.json && cd src/common && npm publish && cd ../.. && npm i -S -P @oly_op/music-app-common@latest"
	},
