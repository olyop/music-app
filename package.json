{
  "private": true,
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "boilerplate",
  "deprecated": false,
  "type": "module",
  "scripts": {
    "build": "rm -r -f src/server/build && clear && webpack --env.mode production",
    "server": "clear && node --experimental-modules src/server/index.js",
    "dev-client": "node_modules/.bin/webpack-dev-server --env.mode development",
    "dev-server": "node_modules/.bin/nodemon --experimental-modules src/server/index.js",
    "development": "rm -r -f src/server/build && clear && concurrently \"npm run dev-client\" \"npm run dev-server\" && clear",
    "sync": "git checkout . && git remote update && git pull",
    "push": "git status && git add -A && git commit -m \"Update\" && git push -u origin master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olyop/boilerplate.git"
  },
  "keywords": [
    "lodash",
    "react",
    "redux",
    "webpack",
    "babel",
    "nodejs",
    "express",
    "mongodb"
  ],
